{
  "version": "0.0.1",
  "private": true,
  "description": "A VuePress plugin for compressing image assets",
  "keywords": [
    "vite",
    "vuepress",
    "vuepress-plugin",
    "imagemin",
    "vben"
  ],
  "homepage": "https://github.com/yjl9903/vuepress-plugin-imagemin/tree/master/#readme",
  "bugs": {
    "url": "https://github.com/yjl9903/vuepress-plugin-imagemin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yjl9903/vuepress-plugin-imagemin"
  },
  "license": "MIT",
  "author": "Vben",
  "scripts": {
    "build": "pnpm run build -C core",
    "build:play": "pnpm run build -C playground",
    "dev:play": "pnpm run dev -C playground",
    "lint:eslint": "eslint \"packages/**/*.{ts,tsx}\" --fix",
    "lint:pretty": "pretty-quick --staged",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "preversion": "pnpm build"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ],
    "packages/*/{src,types}/**/*.ts": [
      "eslint --ext .ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "bumpp": "^7.1.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5",
    "unbuild": "^0.6.8",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  },
  "peerDependencies": {
    "vuepress": ">=2.0.0-beta.36"
  },
  "packageManager": "pnpm@6.32.4"
}
